{
  "name": "Stratbook",
  "version": "1.0.0",
  "description": "A platform for teams to manage their members and strategies.",
  "main": "server.js",
  "scripts": {
    "test": "npm run unit-tests & npm run functional-tests",
    "unit-tests": "mocha --ui tdd testing/unit_tests.js",
    "functional-tests": "mocha --ui tdd testing/functional_tests.js",
    "build": "cd client && npm run build && cd ..",
    "heroku-postbuild": "npm prune --production",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm run start && cd ..",
    "scss": "cd client && npm run scss && cd .."
  },
  "author": "Cameron Burkholder",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bad-words": "^3.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "diacritic": "0.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "googleapis": "^59.0.0",
    "helmet": "^4.4.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.27",
    "morgan": "^1.10.0",
    "nodemailer": "^6.4.16",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "random-words": "^1.1.1",
    "randomatic": "^3.1.1",
    "socket.io": "^3.0.5",
    "socket.io-client": "^4.1.1",
    "validator": "^13.1.1",
    "web-push": "^3.4.4",
    "xmlhttprequest-ssl": ">=1.6.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "husky": "^4.3.8",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "nodemon": "^2.0.4"
  },
  "proxy": "http://localhost:5000"
}
